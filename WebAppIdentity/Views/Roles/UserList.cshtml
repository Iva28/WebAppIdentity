@model IEnumerable<User>


@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h3>Users</h3>
    <table class="table">
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
                </td>
            </tr>
        }
    </table>
</div>
