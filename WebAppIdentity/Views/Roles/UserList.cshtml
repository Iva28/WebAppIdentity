@model IEnumerable<User>


@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Users</h3>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <div class="a"><a asp-action="Edit" asp-route-userid="@user.Id">Access rights</a></div>
            </td>
        </tr>
    }
</table>
<br />
<div class="a"><a asp-action="Index">Back to list</a></div>

